        xmlns:local="clr-namespace:AutoUpdate"
        mc:Ignorable="d"
        Title="AutoUpdater" Height="350" Width="525">
    <Grid>
        <Button x:Name="bUpdate" Content="Update" HorizontalAlignment="Left" Height="54" Margin="53,113,0,0" VerticalAlignment="Top" Width="166" Click="bUpdate_Click"/>
        <Button x:Name="bVersion" Content="Version" HorizontalAlignment="Left" Height="54" Margin="295,113,0,0" VerticalAlignment="Top" Width="166" Click="bVersion_Click"/>
        <Label x:Name="lVersion" Content="Label" HorizontalAlignment="Left" Height="26" Margin="176,46,0,0" VerticalAlignment="Top" Width="132"/>

    </Grid>
